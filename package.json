    "globby": "^11.0.0",
    "jaid-logger": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ora": "^4.0.3",
    "pkg": "^4.4.7",
    "promise-sequence": "^0.3.1",
    "stringify-author": "^0.1.3",
